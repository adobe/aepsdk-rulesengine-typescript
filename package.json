{
  "name": "@adobe/aep-rules-engine",
  "version": "2.0.3",
  "description": "Adobe Experience Platform rules engine.",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aepsdk-rulesengine-typescript.git"
  },
  "type": "module",
  "module": "dist/es/index.js",
  "main": "dist/cjs/index.cjs",
  "types": "types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/es/index.js",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "files": [
    "dist/*",
    "types/*",
    "src/**/!(*.spec).ts"
  ],
  "scripts": {
    "add-license": "node tools/add-license.js",
    "prebuild": "npm run clean && npm run lint",
    "build": "rollup -c",
    "clean": "rimraf dist types",
    "format": "prettier --write \"{src,test}/**/*.{js,cjs,mjs,ts}\" \"**/*.md\"",
    "prelint": "npm run format && npm run add-license",
    "lint": "./node_modules/.bin/eslint && tsc",
    "prepare": "husky",
    "prepublishOnly": "rollup -c",
    "pretty-quick": "pretty-quick",
    "pretest": "npm run lint",
    "test": "NODE_ENV=development jest"
  },
  "author": "Adobe",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.19.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^15.14.0",
    "handlebars": "^4.7.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.6",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "staged-git-files": "^1.3.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "overrides": {
    "eslint": "^9.19.0"
  }
}
